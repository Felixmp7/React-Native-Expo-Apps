{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "baseUrl": "src",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@hooks/*": [
        "./hooks/*"
      ],
      "@screens/*": [
        "./screens/*"
      ],
      "@navigation/*": [
        "./navigation/*"
      ],
    },
  }
}